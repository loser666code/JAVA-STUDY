<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="400.0" styleClass="AnchorPane" stylesheets="@../../resources/css/listagemusers.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ListagemUsersController">
   <children>
      <TextField fx:id="tfPesquisa" layoutX="66.0" layoutY="42.0" prefHeight="35.0" prefWidth="310.0" styleClass="tfPesquisa" stylesheets="@../../resources/css/listagemusers.css" />
      <ImageView fx:id="imgVPesquisar" fitHeight="35.0" fitWidth="40.0" layoutX="26.0" layoutY="42.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../resources/imagens/pesquisarIcon.png" />
         </image>
      </ImageView>
      <Button fx:id="bntCadastrar" layoutX="25.0" layoutY="507.0" mnemonicParsing="false" onAction="#bntCadastrarOnClick" prefHeight="50.0" prefWidth="350.0" styleClass="bnt" stylesheets="@../../resources/css/listagemusers.css" text="Cadastrar User" />
      <TableView fx:id="twUsers" layoutX="10.0" layoutY="87.0" onMouseClicked="#tableViewOnClick" prefHeight="411.0" prefWidth="381.0" styleClass="tableview" stylesheets="@../../resources/css/listagemusers.css">
        <columns>
          <TableColumn prefWidth="75.0" text="Nome" />
          <TableColumn prefWidth="75.0" text="Sobrenome" />
            <TableColumn prefWidth="75.0" text="Email" />
            <TableColumn prefWidth="75.0" text="Cargo" />
            <TableColumn text="Telefone" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
